{
  "name": "react-crestui",
  "version": "0.1.10",
  "description": "React components written in typescript and based on React 16.8.0 functional components and styled-components",
  "author": "crestui",
  "license": "MIT",
  "repository": "crestui/react-crestui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "echo hello",
    "lint": "eslint src",
    "fix": "eslint --fix src && eslint --fix docs",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build"
  },
  "peerDependencies": {
    "merge-options": ">= 2.0.0",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "react-id-generator": ">= 3.0.1",
    "react-is": ">= 16.8.0",
    "react-portal": ">= 4.2.1",
    "react-responsive": ">= 8.1.0",
    "styled-components": ">= 5.1.1",
    "styled-icons": ">= 10.13.0",
    "use-onclickoutside": ">= 0.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "7.21.5",
    "@testing-library/jest-dom": "5.11.2",
    "@testing-library/react": "10.4.7",
    "@testing-library/user-event": "12.0.17",
    "@types/jest": "26.0.7",
    "@types/node": "12.12.38",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-portal": "4.0.2",
    "@types/react-responsive": "8.0.2",
    "@types/styled-components": "5.1.1",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.2",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-react": "9.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-standard": "4.0.1",
    "merge-options": "2.0.0",
    "microbundle-crl": "0.13.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-id-generator": "3.0.1",
    "react-is": "16.13.1",
    "react-portal": "4.2.1",
    "react-responsive": "8.1.0",
    "react-scripts": "3.4.1",
    "styled-components": "5.1.1",
    "styled-icons": "10.13.0",
    "typescript": "3.9.7",
    "use-onclickoutside": "0.3.1"
  },
  "files": [
    "dist"
  ]
}
