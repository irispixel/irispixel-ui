{
  "name": "@irispixel/svelte-canvas",
  "license": "MIT",
  "repository": "git@github.com:irispixel/irispixel-ui.git",
  "version": "0.1.2",
  "types": "package/index.d.ts",
  "author": "IrisPixel",
  "scripts": {
    "clean": "rimraf package && rimraf node_modules/@irispixel",
    "build": "svelte-kit package",
    "package": "svelte-kit package",
    "prepare": "yarn clean && NODE_ENV=production yarn package",
    "test": "jest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "test:watch": "npm run test -- --watch",
    "next": "yarn version patch --deferred && yarn version apply",
    "pub": "yarn prepare && cd package && npm publish --tolerate-republish"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.18.3"
  },
  "dependencies": {
    "@irispixel/common-intersector": "^0.2.5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.18.3",
    "@sveltejs/kit": "^1.0.0-next.352",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "3.1.3",
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^28.1.1",
    "jest-canvas-mock": "^2.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "svelte": "^3.48.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "ts-jest": "^28.0.5"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  }
}
